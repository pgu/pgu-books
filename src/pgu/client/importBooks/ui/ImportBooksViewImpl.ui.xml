<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" 
    xmlns:g="urn:import:com.google.gwt.user.client.ui"
    xmlns:b="urn:import:com.github.gwtbootstrap.client.ui">
    
    <g:HTMLPanel>
        <b:WellForm>
            <b:ControlLabel>Start</b:ControlLabel>
            <b:TextBox ui:field="startBox" placeholder="0"/>
            <b:ControlLabel>Length</b:ControlLabel>
            <b:TextBox ui:field="lengthBox" placeholder="10"/>
            <b:FormActions>
                <b:SubmitButton ui:field="importBtn" addStyleNames="btn" type="PRIMARY">Submit</b:SubmitButton>
            </b:FormActions>
        </b:WellForm>    
        
    </g:HTMLPanel>
</ui:UiBinder> 